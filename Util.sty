\newcommand{\centeredframebox}[3]
{\put(0,0){\line(0,1){#2}}
\put(0,0){\line(1,0){#1}}
\put(#1,#2){\line(0,-1){#2}}
\put(#1,#2){\line(-1,0){#1}}
\makebox(#1,#2){#3}}

