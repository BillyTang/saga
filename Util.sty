\newcommand{\storageTank}[2]{
    \setcounter{step}{#1}
    %left line
    \setcounter{x}{\value{step}*0}
    \setcounter{y}{\value{step}*4}
    \setcounter{length}{\value{step}*12}
    \put(\value{x},\value{y}){\line(0,1){\value{length}}}
    %right line
    \setcounter{x}{\value{step}*20}
    \setcounter{y}{\value{step}*4}
    \setcounter{length}{\value{step}*12}
    \put(\value{x},\value{y}){\line(0,1){\value{length}}}

    %ellipse on top
    \setcounter{x}{\value{step}*0}
    \setcounter{y}{\value{step}*16}
    \setcounter{x1}{\value{step}*0}
    \setcounter{y1}{\value{step}*18}
    \setcounter{x2}{\value{step}*5}
    \setcounter{y2}{\value{step}*19}
    \put(0,0){\qbezier(\value{x},\value{y})(\value{x1},\value{y1})(\value{x2},\value{y2})}
    \setcounter{x}{\value{step}*5}
    \setcounter{y}{\value{step}*19}
    \setcounter{x1}{\value{step}*10}
    \setcounter{y1}{\value{step}*20}
    \setcounter{x2}{\value{step}*15}
    \setcounter{y2}{\value{step}*19}
    \put(0,0){\qbezier(\value{x},\value{y})(\value{x1},\value{y1})(\value{x2},\value{y2})}
    \setcounter{x}{\value{step}*15}
    \setcounter{y}{\value{step}*19}
    \setcounter{x1}{\value{step}*20}
    \setcounter{y1}{\value{step}*18}
    \setcounter{x2}{\value{step}*20}
    \setcounter{y2}{\value{step}*16}
    \put(0,0){\qbezier(\value{x},\value{y})(\value{x1},\value{y1})(\value{x2},\value{y2})}
    \setcounter{x}{\value{step}*0}
    \setcounter{y}{\value{step}*12}
    \put(\value{x},\value{y}){
        \setcounter{x}{\value{step}*0}
        \setcounter{y}{\value{step}*4}
        \setcounter{x1}{\value{step}*0}
        \setcounter{y1}{\value{step}*2}
        \setcounter{x2}{\value{step}*5}
        \setcounter{y2}{\value{step}}
        \put(0,0){\qbezier(\value{x},\value{y})(\value{x1},\value{y1})(\value{x2},\value{y2})}
        \setcounter{x}{\value{step}*5}
        \setcounter{y}{\value{step}}
        \setcounter{x1}{\value{step}*10}
        \setcounter{y1}{\value{step}*0}
        \setcounter{x2}{\value{step}*15}
        \setcounter{y2}{\value{step}}
        \put(0,0){\qbezier(\value{x},\value{y})(\value{x1},\value{y1})(\value{x2},\value{y2})}
        \setcounter{x}{\value{step}*15}
        \setcounter{y}{\value{step}}
        \setcounter{x1}{\value{step}*20}
        \setcounter{y1}{\value{step}*2}
        \setcounter{x2}{\value{step}*20}
        \setcounter{y2}{\value{step}*4}
        \put(0,0){\qbezier(\value{x},\value{y})(\value{x1},\value{y1})(\value{x2},\value{y2})}
        }
    %ellipse on bottom
    \setcounter{x}{\value{step}*0}
    \setcounter{y}{\value{step}*4}
    \setcounter{x1}{\value{step}*0}
    \setcounter{y1}{\value{step}*2}
    \setcounter{x2}{\value{step}*5}
    \setcounter{y2}{\value{step}}
    \put(0,0){\qbezier(\value{x},\value{y})(\value{x1},\value{y1})(\value{x2},\value{y2})}
    \setcounter{x}{\value{step}*5}
    \setcounter{y}{\value{step}}
    \setcounter{x1}{\value{step}*10}
    \setcounter{y1}{\value{step}*0}
    \setcounter{x2}{\value{step}*15}
    \setcounter{y2}{\value{step}}
    \put(0,0){\qbezier(\value{x},\value{y})(\value{x1},\value{y1})(\value{x2},\value{y2})}
    \setcounter{x}{\value{step}*15}
    \setcounter{y}{\value{step}}
    \setcounter{x1}{\value{step}*20}
    \setcounter{y1}{\value{step}*2}
    \setcounter{x2}{\value{step}*20}
    \setcounter{y2}{\value{step}*4}
    \put(0,0){\qbezier(\value{x},\value{y})(\value{x1},\value{y1})(\value{x2},\value{y2})}
    \setcounter{x}{\value{step}*10}
    \setcounter{y}{\value{step}*7}
    \put(\value{x},\value{y}){\makebox(0,0){\tiny{#2}}}
}

