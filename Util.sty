%------------------------------------
% Cylinder with plate
%------------------------------------

\newcommand{\cylinderWithPlate}[1]
{
      %design 1
      \moveto(0,0)
      \circlearc{0}{0}{7}{0}{50}
      \strokepath
      %design 2
      \moveto(0,0)
      \circlearc{0}{0}{7}{50}{100}
      \strokepath
      %design 3
      \moveto(0,0)
      \circlearc{0}{0}{7}{100}{150}
      \strokepath
      %design 4
      \moveto(0,0)
      \circlearc{0}{0}{7}{150}{200}
      \strokepath
      %design 5
      \moveto(0,0)
      \circlearc{0}{0}{7}{200}{250}
      \strokepath
      %design 6
      \moveto(0,0)
      \circlearc{0}{0}{7}{250}{300}
      \strokepath
      %design 7
      \moveto(0,0)
      \circlearc{0}{0}{7}{300}{350}
      \strokepath
      %white gap
      \moveto(0,0)
      \circlearc{0}{0}{7}{350}{350}
      \strokepath
      \color{white}
      \put(0,0){\circle*{12}}
      \color{black}
      %outer circle
      \put(0,0){\circle{12}}
      %center
      \put(0,0){\circle*{1}}
      %arrows
      \moveto(0,5)
      \circlearc{0}{0}{5}{90}{180}
      \strokepath
      \put(-5,0){\vector(0,-1){1}}
      \moveto(5,0)
      \closepath
      \circlearc{0}{0}{5}{360}{270}
      \strokepath
      \put(5,0){\vector(0,1){1}}
      %colordevice
      \color{#1}
      \polygon*(-2,14)(0,7)(2,14)
      \color{black}
      \polygon(-2,14)(0,7)(2,14)
}

