% Centered framecolorbox
%------------------------------------

\newcommand{\centeredframecolorbox}[4]
{\color{#4}
\put(0,0){\rule{0mm}{0mm}}
\put(0,0){\rule{#1\unitlength}{#2\unitlength}}
\color{black}
\put(0,0){\centeredframebox{#1}{#2}{#3}}}

